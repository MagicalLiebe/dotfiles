{
    "Prompt": "[peco]",
    "Keymap": {
        "Esc": "peco.Cancel",
        "Enter": "peco.Finish",
        "BS": "peco.DeleteBackwardChar",
        "C-c": "peco.Cancel",
        "C-f": "peco.ForwardChar",
        "C-a": "peco.BeginningOfLine",
        "C-e": "peco.EndOfLine",
        "C-b": "peco.BackwardChar",
        "C-r": "peco.RotateMatcher",
        "C-k": "peco.SelectUp",
        "C-j": "peco.SelectDown",
        "C-h": "peco.ScrollPageUp",
        "C-l": "peco.ScrollPageDown"
    }
}
